{
  "name": "Nicovideo auto move",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Move new broadcast automatically",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": [
        "http://live.nicovideo.jp/watch/*"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "nicovideo_automove.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
